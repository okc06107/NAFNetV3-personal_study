name: NAFNetV3-final
model_type: ImageRestorationModel
scale: 1
num_gpu: 1
manual_seed: 10

datasets:
  val:
    name: SIDD_val
    type: SiddBenchmarkDataset
    dataroot_gt: /data1/SIDD_raw/valid/valid_gt_srgb_crops
    dataroot_lq: /data1/SIDD_raw/valid/valid_lq_raw_crops
    benchmark_folder: /home/swhong/SIDD_Benchmark_Data
    io_backend:
      type: disk
      
    gt_size: 256

    dataroot_gt_raw: /data1/SIDD_raw/valid/valid_gt_raw_crops

network_g:
  type: NAFNetV3
  input_shuffle: 2
  shuffle: !!int {{shuffle1}}
  width: !!int {{width}}
  ch_mid: !!int {{ch_mid}}
  enc_blk_nums: []
  middle_blk_num: !!int {{mid}}
  dec_blk_nums: []
  ending_blk_type: !!int {{end_type}}
  middle_blk_type: !!int {{mid_type}}
  sca: !!bool {{sca}}

path: 
  pretrain_network_g: ~
  strict_load_g: true
  resume_state: ~

val:
  save_img: false
  grids: false
  use_image: false

  metrics:
    ~